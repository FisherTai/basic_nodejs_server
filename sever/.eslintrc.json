{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "quotes": ["error", "double", { "allowTemplateLiterals": true }],
        "linebreak-style":"off",
        "no-console":"off",
        "no-underscore-dangle":"off"
    }
}
